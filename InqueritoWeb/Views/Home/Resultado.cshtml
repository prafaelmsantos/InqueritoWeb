@model IEnumerable<Resposta>
<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/lib/bootstrap5/dist/css/bootstrap.css">
    <title>Inquerito Web! :: Resultado Parcial</title>
</head>

<body>
    <div class="container">
        <h1>Inquerito Web</h1>
        <h2>Resultado Parcial</h2>
        <h4>Tem coragem de fazer uma viagem a Marte?</h4>
        @foreach (Resposta r in Model)
        {
            <p>@(r.Nome): @(r.Sim.Value ? "Sim" : "Não")</p>
        }
        <p>
            <a asp-action="Index" class="btn btn-primary">Voltar ao Início</a>
        </p>
    </div>
</body>

</html>